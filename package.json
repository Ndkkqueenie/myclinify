		"yup": "0.29.3"
	},
	"resolutions": {
		"graphql": "15.3.0"
	},
	"scripts": {
		"start:client": "react-scripts start",
		"start": "node src/server/index.js",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint './src/**/*.{ts,tsx}' --ext .ts,.tsx",
		"lint:fix": "eslint './src/**/*.{ts,tsx}' --ext .tsx,.ts --fix",
		"lint:e2e": "eslint './e2e-tests' --fix",
		"apollo:generate": "apollo client:codegen --outputFlat --localSchemaFile=graphql-schema.json --target=typescript --includes=src/**/*.ts --excludes=src/apollo/*.ts --tagName=gql src/graphql-types",
		"apollo:download": "apollo client:download-schema --endpoint https://my-clinify-api-stag.herokuapp.com/graphql graphql-schema.json",
		"schema:check": "rm -rf src/graphql-types && npm run apollo:download && npm run apollo:generate",
		"e2e:all": "testcafe chrome e2e-tests/features -e --assertion-timeout 35000 --selector-timeout 35000 --pageload-timeout 35000",
		"e2e-headless:all": "testcafe chrome:headless e2e-tests/features -e --assertion-timeout 35000 --selector-timeout 35000 --pageload-timeout 35000",
		"e2e:one": "testcafe chrome e2e-tests/features/$npm_config_path -e --debug-on-fail --assertion-timeout 35000 --selector-timeout 35000 --pageload-timeout 35000",
		"e2e-headless:one": "testcafe chrome:headless e2e-tests/features/$npm_config_path -e --assertion-timeout 35000 --selector-timeout 35000 --pageload-timeout 35000"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run apollo:download && git add ."
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/helper-builder-react-jsx-experimental": "^7.12.11",
		"@types/chance": "1.1.1",
		"@types/graphql": "14.5.0",
		"@types/react-redux": "7.1.9",
		"@typescript-eslint/eslint-plugin": "4.5.0",
		"@typescript-eslint/parser": "4.5.0",
		"concurrently": "5.0.2",
		"eslint": "7.12.0",
		"eslint-config-airbnb": "18.2.0",
		"eslint-config-prettier": "6.14.0",
		"eslint-import-resolver-typescript": "2.3.0",
		"eslint-plugin-import": "2.22.1",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.21.5",
		"husky": "4.2.5",
		"lint-staged": "10.2.11",
		"prettier": "2.1.2",
		"react-refresh": "0.8.3",
		"testcafe": "1.10.1",
		"testcafe-reporter-xunit": "2.1.0"
	},
	"lint-staged": {
		"**/*.{ts,js,tsx,json,md}": [
			"npm run --silent lint:fix"
		]
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"semi": true,
		"singleQuote": true,
		"useTabs": false,
		"tabWidth": 2,
		"printWidth": 100,
		"trailingComma": "all"
	}
}
